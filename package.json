{
  "name": "carrot-audit-rules",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "build-lambda": "nx build-lambda",
    "build-lambda:affected": "nx affected --target build-lambda",
    "build-lambda:all": "nx run-many --target build-lambda",
    "commit": "czg",
    "lint": "nx lint",
    "lint:affected": "nx affected --target lint --fix",
    "lint:all": "nx run-many --target lint --fix",
    "pkgJsonLint": "npmPkgJsonLint .",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky && ts-patch install && typia patch",
    "test": "nx test",
    "test:affected": "nx affected --target test",
    "test:all": "nx run-many --target test",
    "ts": "nx ts",
    "ts:affected": "nx affected --target ts",
    "ts:all": "nx run-many --target ts"
  },
  "private": true,
  "packageManager": "pnpm@8.15.8",
  "engines": {
    "node": "18.19.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
