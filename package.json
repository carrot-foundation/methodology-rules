{
  "name": "carrot-methodology-rules",
  "version": "0.0.0",
  "license": "LGPL-3.0",
  "scripts": {
    "apply-methodology-rule": "node tools/apply-methodology-rule.js",
    "build-lambda": "nx build-lambda",
    "build-lambda:affected": "nx affected --target build-lambda",
    "build-lambda:all": "nx run-many --target build-lambda",
    "commit": "czg",
    "create-rule": "node tools/create-rule.js",
    "fix-project-format": "node scripts/fix-project-json-format.js",
    "lint": "nx lint",
    "lint:affected": "nx affected --target lint --fix",
    "lint:all": "nx run-many --target lint --fix",
    "pkgJsonLint": "npmPkgJsonLint .",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky && ts-patch install && typia patch",
    "test": "nx test",
    "test:affected": "nx affected --target test",
    "test:all": "nx run-many --target test",
    "ts": "nx ts",
    "ts:affected": "nx affected --target ts",
    "ts:all": "nx run-many --target ts",
    "update-tags": "node scripts/update-project-tags.js",
    "update-waste-classification": "node tools/update-waste-classification.js",
    "upload-lambdas-and-metadata": "./scripts/upload-lambdas-and-metadata.sh"
  },
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "engines": {
    "node": "22.15.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
